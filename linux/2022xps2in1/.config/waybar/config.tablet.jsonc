{
  "height": 25,
    "modules-left": ["custom/menu", "sway/workspaces", "custom/scratchpad"],
    "modules-center": [],
    "modules-right": [
        // informational
        "custom/clipboard",
        "custom/perfmode",
        "battery",
        // media
        "custom/playerctl",
        "idle_inhibitor",
        "pulseaudio",
        "backlight",

        // system
        "tray",
        "clock"
